{
  "name": "global-maintainers-summit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt --port 5000",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest",
    "e2e": "TZ=America/New_York cypress open --browser chrome",
    "e2e:ci": "TZ=America/New_York cypress run",
    "component": "TZ=America/New_York cypress open-ct --browser chrome",
    "component:ci": "TZ=America/New_York cypress run-ct",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "calendar-link": "^2.1.1",
    "core-js": "^3.12.1",
    "focus-trap": "^6.4.0",
    "focus-trap-vue": "^1.1.1",
    "nuxt": "^2.15.5",
    "vue-jest": "^3.0.7"
  },
  "devDependencies": {
    "@cypress/vue": "^2.2.2",
    "@cypress/webpack-dev-server": "^1.1.6",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/svg": "^0.1.12",
    "@testing-library/cypress": "^7.0.5",
    "@testing-library/vue": "^5.6.2",
    "@vue/test-utils": "^1.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cypress": "^7.2.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "sass": "^1.32.11",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "webpack-dev-server": "^3.11.2"
